#include <stdio.h>
int main(void) {
	int t;
	scanf("%d",&t);
	for(int i=0;i<t;i++){
	    int n;
	    int c=0;
	    scanf("%d",&n);
	    int a[n];
	    for(int i=0;i<n;i++){
	        scanf("%d",&a[i]);
	        if(a[i]>4){
	            c++;
	        }
	    }
	    if(c==n){
	        printf("yes\n");
	    }
	    else{
	        printf("no\n");
	    }
	}

}

