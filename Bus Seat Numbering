#include <stdio.h>
int main(void) {
	int t;
	scanf("%d",&t);
	int n;
	for(int i=0;i<t;i++){
	    scanf("%d",&n);
	    if(n>=1 && n<=10)
	    printf("Lower Double\n");
	    else if(n>=11 && n<=15)
	    printf("Lower Single\n");
	    else if(n>=16 && n<=25)
	    printf("Upper Double\n");
	    else
	    printf("Upper Single\n");
	}

}

