#include <stdio.h>
int main(void) {
	int t;
	scanf("%d",&t);
	for(int k=0;k<t;k++){
	    int n,k;
	    int count=0;
	    scanf("%d%d",&n,&k);
	    int a[n];
	    for(int i=0;i<n;i++){
	        scanf("%d",&a[i]);
	    }
	    for(int i=0;i<n;i++){
	        if(a[i]>=k){
	            count++;
	        }
	    }
	    printf("%d\n",count);
	}

}

