#include <stdio.h>
int main(void) {
	int t;
	scanf("%d",&t);
	int a,b,c,d;
	for(int i=0;i<t;i++){
	    int p,q;
	    scanf("%d%d%d%d",&a,&b,&c,&d);
	    if(a>b){
	        p=a;
	    }else{
	        p=b;
	    }
	    if(c>d){
	        q=c;
	    }else{
	        q=d;
	    }
	    if(p>q){
	        printf("Q\n");
	    }
	    else if(p<q){
	        printf("P\n");
	    }
	    else{
	        printf("TIE\n");
	    }
	}

}

