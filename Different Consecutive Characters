#include <stdio.h>
int main(void) {
	int t;
	scanf("%d",&t);
	for(int i=0;i<t;i++){
	    int n,c=0;
	    scanf("%d",&n);
	    char a[n];
	    scanf("%s",a);
	    for(int i=0;a[i];i++){
	        if((a[i]=='1' && a[i+1]=='1') || (a[i]=='0' && a[i+1]=='0')){
	            c++;
	        }
	    }
	    printf("%d\n",c);
	}

}

